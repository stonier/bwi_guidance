<launch>
  <arg name="map_file" default="$(find utexas_gdc)/maps/3ne-real-new.yaml" />
  <arg name="door_file" default="$(find clingo_interface)/config/real_doors.yaml" />
  <arg name="location_file" default="$(find clingo_interface)/config/real_locations.yaml" />
  <node name="clingo_interface_gui" type="clingo_interface_gui" pkg="clingo_interface_gui">
    <param name="map_file" value="$(arg map_file)" />
    <param name="door_file" value="$(arg door_file)" />
    <param name="location_file" value="$(arg location_file)" />
  </node>
</launch>
